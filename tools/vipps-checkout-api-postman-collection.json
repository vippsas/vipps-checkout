{
	"info": {
		"_postman_id": "2582ee7c-971d-44f7-8fa5-741abea5e9da",
		"name": "Checkout API v2 - Collection",
		"description": "For details, see the [Checkout API Guide](https://vippsas.github.io/vipps-developer-docs/docs/APIs/checkout-api).",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "v2/session",
			"item": [
				{
					"name": "Create a Checkout session",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if(pm.response.code === 200) { \r",
									"    var jsonData = pm.response.json();\r",
									"    var str = jsonData.pollingUrl\r",
									"    var str2 = str.split('session/')[1]\r",
									"    pm.environment.set('sessionId', str2);\r",
									"}"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"pm.environment.set('orderId',pm.environment.get('merchantSerialNumber') + (new Date()).getTime());"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Vipps-System-Name",
								"value": "{{vipps_system_name}}"
							},
							{
								"key": "Vipps-System-Version",
								"value": "{{vipps_system_version}}"
							},
							{
								"key": "Vipps-System-Plugin-Name",
								"value": "{{vipps_system_plugin_name}}"
							},
							{
								"key": "Vipps-System-Plugin-Version",
								"value": "{{vipps_system_plugin_version}}"
							},
							{
								"key": "client_id",
								"value": "{{client_id}}"
							},
							{
								"key": "client_secret",
								"value": "{{client_secret}}"
							},
							{
								"key": "Ocp-Apim-Subscription-Key",
								"value": "{{Ocp-Apim-Subscription-Key}}"
							},
							{
								"key": "Merchant-Serial-Number",
								"value": "{{merchantSerialNumber}}"
							},
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Accept",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"merchantInfo\": {\n    \"callbackPrefix\": \"{{callbackPrefix}}\",\n    \"returnUrl\": \"{{fallBack}}\",\n    \"callbackAuthorizationToken\": \"{{$guid}}\",\n    \"termsAndConditionsUrl\": \"{{termsAndConditionsUrl}}\"\n  },\n  \"transaction\": {\n    \"amount\": {\n      \"currency\": \"{{currency}}\",\n      \"value\": \"{{amount}}\"\n    },\n    \"reference\": \"{{orderId}}\",\n    \"paymentDescription\": \"{{transactionTextInitiate}}\"\n  },\n  \"logistics\": {\n    \"dynamicOptionsCallback\": \"{{dynamicOptionsCallbackUrl}}\",\n    \"fixedOptions\": [\n      {\n        \"amount\": {\n          \"currency\": \"{{currency}}\",\n          \"value\": \"{{amount}}\"\n        },\n        \"id\": \"postenservicepakke1\",\n        \"priority\": 1,\n        \"brand\": \"Posten\",\n        \"product\": \"Package to home\",\n        \"isDefault\": true,\n        \"description\": \"Your package is delivered to your home.\",\n        \"isPickupPoint\": false,\n        \"isPorterbuddy\": false,\n      },\n      {\n        \"amount\": {\n          \"currency\": \"{{currency}}\",\n          \"value\": \"{{amount}}\"\n        },\n        \"id\": \"postenservicepakke2\",\n        \"priority\": 2,\n        \"brand\": \"Posten\",\n        \"product\": \"Package to pickup point\",\n        \"isDefault\": false,\n        \"description\": \"Pick up your package at a pickup point\",\n        \"isPickupPoint\": true,\n        \"isPorterbuddy\": false,\n      }\n    ]\n  },\n  \"prefillCustomer\": {\n    \"phoneNumber\": \"{{internationalMobileNumber}}\",\n    \"firstName\": \"{{firstName}}\",\n    \"lastName\": \"{{lastName}}\",\n    \"email\": \"{{email}}\",\n    \"streetAddress\": \"{{streetAddress}}\",\n    \"city\": \"{{city}}\",\n    \"postalCode\": \"{{postalCode}}\",\n    \"country\": \"{{country}}\"\n  },\n  \"customerInteraction\": \"Customer_not_present\",\n  \"contactFields\": true,\n  \"addressFields\": true,\n  \"userFlow\": \"NATIVE_REDIRECT\"\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{base_url}}/checkout/v2/session",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"checkout",
								"v2",
								"session"
							]
						},
						"description": "Creates a checkout session and returns a JWT token which wraps the login URL,\r\necom landing page URL, free standing card page URL."
					},
					"response": []
				},
				{
					"name": "Get session info",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Vipps-System-Name",
								"value": "{{vipps_system_name}}"
							},
							{
								"key": "Vipps-System-Version",
								"value": "{{vipps_system_version}}"
							},
							{
								"key": "Vipps-System-Plugin-Name",
								"value": "{{vipps_system_plugin_name}}"
							},
							{
								"key": "Vipps-System-Plugin-Version",
								"value": "{{vipps_system_plugin_version}}"
							},
							{
								"key": "client_id",
								"value": "{{client_id}}"
							},
							{
								"key": "client_secret",
								"value": "{{client_secret}}"
							},
							{
								"key": "Ocp-Apim-Subscription-Key",
								"value": "{{Ocp-Apim-Subscription-Key}}"
							},
							{
								"key": "Merchant-Serial-Number",
								"value": "{{merchantSerialNumber}}"
							},
							{
								"key": "Accept",
								"value": "application/json"
							}
						],
						"url": {
							"raw": "{{base_url}}/checkout/v2/session/:sessionId",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"checkout",
								"v2",
								"session",
								":sessionId"
							],
							"variable": [
								{
									"key": "sessionId",
									"value": "{{sessionId}}",
									"description": "(Required) The Id of the session. Example: \"v52EtjZriRmGiKiAKHByK2\"."
								}
							]
						},
						"description": "Transaction information, user information and shipping information are included in the response\r\nif the SessionState is PaymentInitiated"
					},
					"response": []
				},
				{
					"name": "Cancel current session",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Vipps-System-Name",
								"value": "{{vipps_system_name}}"
							},
							{
								"key": "Vipps-System-Version",
								"value": "{{vipps_system_version}}"
							},
							{
								"key": "Vipps-System-Plugin-Name",
								"value": "{{vipps_system_plugin_name}}"
							},
							{
								"key": "Vipps-System-Plugin-Version",
								"value": "{{vipps_system_plugin_version}}"
							},
							{
								"key": "client_id",
								"value": "{{client_id}}"
							},
							{
								"key": "client_secret",
								"value": "{{client_secret}}"
							},
							{
								"key": "Ocp-Apim-Subscription-Key",
								"value": "{{Ocp-Apim-Subscription-Key}}"
							},
							{
								"key": "Merchant-Serial-Number",
								"value": "{{merchantSerialNumber}}"
							},
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"sessionId\": \"{{sessionId}}\"\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{base_url}}/checkout/v2/session/cancel",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"checkout",
								"v2",
								"session",
								"cancel"
							]
						}
					},
					"response": []
				}
			]
		}
	]
}